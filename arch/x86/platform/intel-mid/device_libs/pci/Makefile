obj-$(subst m,y,$(CONFIG_INPUT_INTEL_MID_VIBRA_PF450CL))                += platform_mid_vibra.o
obj-$(subst m,y,$(CONFIG_USB_XHCI_HCD))					+= platform_ush_hsic.o
obj-$(subst m,y,$(CONFIG_VIDEO_CSS2600))				+= platform_css2600.o
